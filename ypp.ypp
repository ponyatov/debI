%{
#include "hpp.hpp"
%}
%defines %union { Sym*o; }
%token <o> SYM
%token <o> L R
%token <o> EQ
%type <o> ex list
%%
REPL	: | REPL ex	{ W($2); W("\n"); } ;
ex		: SYM
		| L list R	{ $$=$2; }
		| ex EQ ex	{ $$=$2; $$->push($1); $$->push($3); }
;
list	: 			{ $$=new List(); }
		| list ex	{ $$=$1; $$->push($2); }
;
%%	 
